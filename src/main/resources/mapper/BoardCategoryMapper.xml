<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.BoardCategoryMapper">
	<select id="selectList" resultType="vo.BoardCategory">
	select * from tbl_board_category
	</select>
	<select id="selectOne" resultType="vo.BoardCategory">
	select * from tbl_board_category where cno = #{cno}
	</select>
</mapper>
